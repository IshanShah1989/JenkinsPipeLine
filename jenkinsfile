pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
       stage('clean') {
            steps {
                sh "mvn clean install"
            }
        }
	stage('Deploy') {
            steps {
                echo 'Testing Deployment'
            }
        }
        stage('Test') {
            steps {
                sh "mvn test"
            }
        }
    }
}
